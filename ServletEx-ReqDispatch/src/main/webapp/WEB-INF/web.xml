<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>RequestDispatcher</display-name>
  <welcome-file-list>
    <welcome-file>hello.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>header</servlet-name>
  <servlet-class>com.india.servlets.HeaderServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>header</servlet-name>
  <url-pattern>/header.jlc</url-pattern>
  </servlet-mapping>

  <servlet>
  <servlet-name>hello</servlet-name>
  <servlet-class>com.india.servlets.HelloServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>hello</servlet-name>
  <url-pattern>/hello.jlc</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>hai</servlet-name>
  <servlet-class>com.india.servlets.HaiServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>hai</servlet-name>
  <url-pattern>/hai.jlc</url-pattern>
  </servlet-mapping>
</web-app>