package com.lib.book.shop.tags;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import javax.servlet.jsp.JspException;
import javax.servlet.jsp.tagext.TagSupport.pageContext

import org.apache.log4j.Logger;

public class ValidateSessionTag {
	Logger log = Logger.getLogger(this.getClass());

public int doEndTag() throws JspException{
	try {
		HttpServletRequest req=(HttpServletRequest)PageContext.REQUEST;
		HttpServletResponse resp=(HttpServletResponse)PageContext.RESPONSE;		
	}catch (Exception e) {
		// TODO: handle exception
	}
}
}
